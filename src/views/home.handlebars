<div>
  <h1>Lista de productos: </h1>
</div>

{{#each products}}
  <div class="{{this.id}}">
    <h2>{{this.title}}</h2>
    <p>Id: {{this.id}}</p>
    <p>Descripcion: {{this.description}}</p>
    <p><strong>Precio: ${{this.price}}</strong></p>
    <p>Stock disponible: {{this.stock}}</p>
  </div>
{{/each}}

<script src="/js/index.js"></script>